<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:flatui="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal" >

    <RelativeLayout 
         android:layout_width="0dp"
        android:layout_height="37dp"
        android:layout_weight="1">
	    <com.aplex.aplextest.flatui.view.FlatTextView
	        android:layout_centerInParent="true"
	        android:id="@+id/test_time"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:textSize="15sp"
	        android:textStyle="bold"
	        flatui:fl_fontExtension="ttf"
	        flatui:fl_fontFamily="roboto" />
	</RelativeLayout>
    <TextView
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:background="@color/sky_light" />

    <RelativeLayout 
         android:layout_width="0dp"
        android:layout_height="37dp"
        android:layout_weight="1">
	    <com.aplex.aplextest.flatui.view.FlatTextView
	        android:layout_centerInParent="true"
	        android:id="@+id/test_success"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:textSize="15sp"
	        android:textStyle="bold"
	        flatui:fl_fontExtension="ttf"
	        flatui:fl_fontFamily="roboto" />
	</RelativeLayout>
    

    <TextView
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:background="@color/sky_light" />

    <RelativeLayout 
         android:layout_width="0dp"
        android:layout_height="37dp"
        android:layout_weight="1">
	    <com.aplex.aplextest.flatui.view.FlatTextView
	        android:layout_centerInParent="true"
	        android:id="@+id/test_error_param"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:textSize="15sp"
	        android:textStyle="bold"
	        flatui:fl_fontExtension="ttf"
	        flatui:fl_fontFamily="roboto" />
	</RelativeLayout>
    

    <TextView
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:background="@color/sky_light" />

    
    <RelativeLayout 
         android:layout_width="0dp"
        android:layout_height="37dp"
        android:layout_weight="1">
	    <com.aplex.aplextest.flatui.view.FlatTextView
	        android:layout_centerInParent="true"
	        android:id="@+id/test_error_memadd"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:textSize="15sp"
	        android:textStyle="bold"
	        flatui:fl_fontExtension="ttf"
	        flatui:fl_fontFamily="roboto" />
	</RelativeLayout>
   

    <TextView
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:background="@color/sky_light" />

    
    <RelativeLayout 
         android:layout_width="0dp"
        android:layout_height="37dp"
        android:layout_weight="1">
	    <com.aplex.aplextest.flatui.view.FlatTextView
	        android:layout_centerInParent="true"
	        android:id="@+id/test_error_memrw"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:textSize="15sp"
	        android:textStyle="bold"
	        flatui:fl_fontExtension="ttf"
	        flatui:fl_fontFamily="roboto" />
	</RelativeLayout>

</LinearLayout>